<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@500&display=swap" rel="stylesheet">
    <title>Create password</title>
    <style>
        body{
            background-image: url(/css/leftback.png);
            text-align: center;
            display: flex;
            align-items: center;
            justify-content: center;
	margin: 0;
	padding: 0;
    

	min-height: 100vh;
	font-family: 'Jost';
}


.main3{
    width: 50%;
    height: 100hv;

    text-align: center;
            display: flex;
            align-items: center;
            justify-content: center;
	
    
    
    transition: transform 0.3s ease;
}


a{
	position: relative;
	left: 70%;
	top: 20%;
	color: #ffffff;
    transition: transform 5s ease;
	border-radius: 5px;
	font-size: large;
	

}
a:link { text-decoration: none; }
a:visited { text-decoration: none; }
a:hover{
	font-size:large;
	/* background-color: #ffffff; */
	color: rgb(12, 12, 12);
}
.header{
	font-family: 'Jost', sans-serif;
	font-size: xx-large;
	display: flex;
	justify-content: center;
	margin-bottom: 20%;
	margin-top: 20%;
}

label{
    display: block;
            color: #fff;
            font-size: 2em;
            margin: 13% auto;
            font-weight: bold;
            cursor: pointer;
            transition: 0.5s ease-in-out;
}
input{
    width: 70%;
            max-width: 100%;
            height: auto;
            background: rgba(255, 255, 255);
            margin: 3vh auto;
            padding: 10px;
            border: solid rgb(255, 255, 255);
            outline: none;
            border-radius: 5px;
            transition: 0.5s;
            color: #000000;

}
button{
	background:  #00A86B;
	
	height: 40px;
	margin: 30px auto;
	justify-content: center;
	display: block;
	margin-top: 20px;
            color: #fff;
            border: none;
            border-radius: 10px;
            padding: 12px 20px;
            font-size: 16px;
            cursor: pointer;
            transition: background 0.3s;
            font-weight: bold;
}

button:hover{
    background: #e74c3c;

            transform: scale(1.04);
            font-weight: normal;
}
input:hover{
    transform: scale(1.04);
    border:solid  rgb(77, 110, 157);
    border-radius: 10px; 

}
::placeholder{
    color:rgb(57, 57, 57);
    
}
.left{
    text-align: center;
            display: flex;
            align-items: center;
            justify-content: center;
        height: 100vh;
        width: 50%;
}
.box{
    width: 36%;
    min-width: 50px;
            background-color: #2D4A90;
    box-shadow: 0 0 20px rgba(0, 0, 0, 0.5);
	 border-radius: 30px;
}
.i1{
    position: absolute;
    left: 11vw;
}
.visi{
            display: flex;
            flex-direction: row;
            text-align: center;
            align-items: center;
            justify-content: center;
        }
        #eyeicon1{
            width: 8%;
            height: 8%;
            position: relative;
            right: 12%;
        }
        #eyeicon2{
            width: 8%;
            height: 8%;
            position: relative;
            right: 12%;
        }
        #password1{
            position: relative;
            left: 4%;
        }
        #password2{
            position: relative;
            left: 4%;
        }
        .i1{
            height: 73%;
            width: 50%;
        }
    </style>
</head>
<body>
    
        <div class="left">
            <img  class="i1" data-aos="fade-right" src="/css/ambulance.gif" alt="">
    
        </div>
        <div class="main3" data-aos="fade-left">
            <div class="box">
                <label>Step-3/3</label>
                <form action="/register/sendotp/enterotp/password" method="post">
                    <div class="visi">
                    <input type="password" id="password1" placeholder="Create password" name="userpassword">
                    <img src="/css/eye-close.png" id="eyeicon1">
                    </div>
                    <div class="visi">
                    <input type="password" id="password2" placeholder="Confirm password" name="usercpassword">
                    <img src="/css/eye-close.png" id="eyeicon2">
                    </div>
                    <button type="submit" onclick="checking()">Register</button>
                </form>
            </div>
           
        </div>
    
    
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        function checking(){
            if(document.getElementById("password1").value != document.getElementById("password2").value){
            alert("Both passwords are not matching");
        }
        }
        
    </script>
    <script>
        let eyeicon1 = document.getElementById("eyeicon1");
  let password1 = document.getElementById("password1");

        eyeicon1.onclick  = function(){
            if(password1.type == "password"){
                password1.type = "text";
                eyeicon1.src= "/css/eye-open.png"
            }else{
                password1.type = "password"
                eyeicon1.src= "/css/eye-close.png"

            }
        }

    </script>

<script>
    let eyeicon2 = document.getElementById("eyeicon2");
let password2 = document.getElementById("password2");

    eyeicon2.onclick  = function(){
        if(password2.type == "password"){
            password2.type = "text";
            eyeicon2.src= "/css/eye-open.png"
        }else{
            password2.type = "password"
            eyeicon2.src= "/css/eye-close.png"

        }
    }

</script>
    <script>
        AOS.init({
            duration:1000
        });
      </script>
</body>
</html>