<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">

    <title>Hospital Admin Registration</title>
    <style>
       
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            background-image: url('css/backgrou.jpg'); 
            background-size: cover;
            background-position: center;
            /* background-color: #20CDC1; */
            /* opacity: 50%; */
        }

        .navbar {
            background-color: #2D4A90; 
            overflow: hidden;
            display: flex;
        }

        .navbar a {
            float: right;
            /* display: block; */
            color: #fff;
            height: auto;
            text-align: center;
            padding: 16px 16px;
            text-decoration: none;
        }

        .navbar a:hover {
            background-color: #D6233D; 
            transition: background-color 0.3s;
        }

        
        .container {
            display: flex;
            justify-content: center;
            align-items: center;
            min-height: 50vh;
            margin-top: 3vh;
            border-radius: 15px;
            
        }

        .registration-form {
            background: rgba(255, 255, 255, 0.8);
            border-radius: 15px;
            box-shadow: 0 px 10px rgba(131, 40, 40, 0.1);
            width: 90%; 
            max-width: 400px;
            padding: 20px;
            animation: fadeIn 1s ease-in-out; 
            border: solid #2D4A90;
        }

        .form-group {
            margin-bottom: 20px;
        }

        .form-group label {
            display: block;
            font-weight: bold;
            color: #2D4A90; 
        }

        .form-group input {
            width: 100%;
            padding: 5px;
            border: 1px solid #ccc;
            border-radius: 8px;
            transition: all 0.3s; 
            box-sizing: border-box;
        }

        button[type="submit"] {
            background-color: #00A86B; 
            color: #fff;
            cursor: pointer;
            display: block;
            width: 50%; 
            margin-left: 25%;
            margin-right: 25%;
            border: none;
            border-radius: 9px;
            transition: 0.3s ease-in;
            font-weight: 700;
            font-size: medium;
            height: 5vh;
        }

        button[type="submit"]:hover {
            background-color: #E83753; 

            letter-spacing: 0.4vh;
            font-weight: 600;
        }

        button[type="submit"]:active {
            background-color: #BF2F49; 
            transform: scale (1.2);
        }

        .form-group input[type="text"]:focus,
        .form-group input[type="password"]:focus,
        .form-group input[type="email"]:focus {
            border-color: #008bef; 
            border: solid;
            transform: scale(1.02);
            border-radius: 10px;
            outline: none;
            color: rgb(11, 2, 84);
            font-size: large;
        }

        .form-group input[type="text"]:hover,
        .form-group input[type="tel"]:hover,
        .form-group input[type="password"]:hover,
        .form-group input[type="email"]:hover {
            outline: none;
            border: solid;

            border-color:  #2D4A90; 
            transform: scale(1.02); 
            border-radius: 10px;
        }

        
        @media (max-width: 768px) {
            .registration-form {
                width: 90%;
                max-width: 90%;
            }

            .p1 {
                font-size: 5vw;
                margin-top: 10vh;
            }

            
        }

       
        @keyframes fadeIn {
            from {
                opacity: 0;
            }
            to {
                opacity: 1;
            }
        }

        @keyframes slideUpAndFadeOut {
            0% {
                opacity: 1;
                transform: translateY(0);
            }
            100% {
                opacity: 0;
                transform: translateY(-100%);
            }
        }

       
        #logout {
            transition: transform 0.3s, background-color 0.3s, border-radius 0.009s;
            font-size: 3vh;
            text-align: left;
            height: auto;
            padding-top: 3.2vh;
        }

        #logout:hover {
            transform: scale(1.1);
            background-color: #ffffff; 
            color: #20CDC1;
            font-weight: bolder;
            border-radius: 30px;
        }

        #logout:active {
            transform: scale(1.1);
            background-color: #00A86B; 
            color: red;
        }
        .p1{
            padding-left: 10%;
            text-align: center;
            font-size: 4vh;
            color: #fff;
            width: 80%;
        
        }
        .p2{
            font-size: 3.3vh;
        }
        .i1{
            position: absolute;
            width: 12%;
            right: 10vh;

        }
        .i2{
            position: absolute;
            width: 12%;
            right: 40vh;

            
        }
         .i3{
            position: absolute;
            width: 13%;
            left: 40vh;

            
        } 
        .i4{
            position: absolute;
            width: 12%;
            left: 10vh; 
        }
        .visi{
            display: flex;
            flex-direction: row;
            text-align: center;
            align-items: center;
            justify-content: center;
            width: 105%;

        }
        #eyeicon{
            width: 6%;
            height: 6%;
            position: relative;
            right: 10%;
        }
        #eyeicon2{
            width: 6%;
            height: 6%;
            position: relative;
            right: 10%;
        }
        #password{
            position: relative;
            left: 0%;
            width: 100%;
        }

    </style>
</head>
<body>
    <div class="navbar">
    <h1 class="p1">ADMIN</h1>
        <a href="/logout" id="logout">Logout</a>
    </div>
    <div class="container">
        <img  class="i1"  src="/css/doctors 1.png" alt="">
        <img  class="i2"  src="/css/doctors 2.png" alt="">
        <img  class="i3"  src="/css/doctors 3.png" alt="">
        <img  class="i4"  src="/css/doctors 4.png" alt="">

        <div  class="registration-form">
            <h1 class="p2" >Hospital Registration</h2>
            <form action="/adminprofile" method="post">
                <div class="form-group">
                    <label for="hosname">Hospital Name:</label>
                    <input type="text" id="hosname" name="hosname" required>
                </div>
                <div class="form-group">
                    <label for="email">Email:</label>
                    <input type="email" id="email" name="hosemail" required>
                </div>
                <div class="form-group">
                    <label for="phnumber">Phone Number:</label>
                    <input type="tel" id="phnumber" name="hosnumber" required>
                </div>
                <div class="form-group">
                    <label for="lat">Latitude:</label>
                    <input type="tel" id="lat" name="hoslat" required>
                </div>
                <div class="form-group">
                    <label for="lng">Longitude:</label>
                    <input type="tel" id="lng" name="hoslng" required>
                </div>
                <div class="form-group">
                    <label for="password">Create Password:</label>
                    <div class="visi">
                        <input type="password" id="password" name="hospassword" required>
                    <img src="/css/eye-close.png" id="eyeicon">
                    </div>
                </div>
                <div class="form-group">
                    <label for="confirm_password">Confirm Password:</label>
                    <div class="visi">
                    <input type="password" id="confirm_password" name="hoscpassword" required>

                    <img src="/css/eye-close.png" id="eyeicon2">
                    </div>
                </div>
                <div class="buttonclass">
                    <button type="submit" onclick="checking()">Register</button>
                </div>
            </form>
        </div>
    </div>
    <script>
        function checking(){
            alert("A NEW HOSPITAL HAS BEEN REGISTERED !")
        }
    </script>
    <script>
        let eyeicon = document.getElementById("eyeicon");
  let password = document.getElementById("password");

        eyeicon.onclick  = function(){
            if(password.type == "password"){
                password.type = "text";
                eyeicon.src= "/css/eye-open.png"
            }else{
                password.type = "password"
                eyeicon.src= "/css/eye-close.png"

            }
        }

    </script>

<script>
    let eyeicon2 = document.getElementById("eyeicon2");
let confirm_password = document.getElementById("confirm_password");

    eyeicon2.onclick  = function(){
        if(confirm_password.type == "password"){
            confirm_password.type = "text";
            eyeicon2.src= "/css/eye-open.png"
        }else{
            confirm_password.type = "password"
            eyeicon2.src= "/css/eye-close.png"

        }
    }

</script>
    
    <script>
        document.getElementById("logout").addEventListener("click", function() {
            const registrationForm = document.querySelector(".registration-form");
            registrationForm.style.animation = "slideUpAndFadeOut 1.5s ease-in-out";
            setTimeout(function() {
                location.href = "/logout";
            }, 1000);
        });
    </script>
    <script>
        AOS.init({
            duration: 1000
        });
    </script>
</body>
</html>